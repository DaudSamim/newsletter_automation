{
	"manifest_version": 2,
	"name": "Newsletter",
	"version": "0.001",
	"content_scripts":[
		{
			"matches":[
				"<all_urls>"
			],
			"js" : ["content.js"]
		}
	],
	"content_security_policy": "script-src 'self' https://cdn.jsdelivr.net/npm/p5@1.4.0/lib/p5.min.js; object-src 'self'",
	"browser_action": {
	    "default_popup": "popup.html",
	    "default_title": "Newsletter"
    },
    "permissions": ["management","tabs","notifications"]
  

}